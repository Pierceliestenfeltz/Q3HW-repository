import java.util.Scanner;

class recursion{
    
    
    static void printFun(int test){
        if (test < 1)
            return;
 
        else {
            System.out.printf("%d ", test);
 
            printFun(test - 1);
 
            System.out.printf("%d ", test);
            return;
        }
    }
    
    public static void main(String[] args){
        Scanner mrstonesworldofcode2 = new Scanner(System.in);
        int test = mrstonesworldofcode2.nextInt();
        printFun(test);
    }
}
